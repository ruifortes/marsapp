@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

{{!--
	.{{baseClass}} {
		line-height: 1em;
		display: inline-block;
	}


	.{{baseClass}}:before {
		font-family: {{fontName}} !important;
		font-style: normal;
		font-weight: normal !important;
		vertical-align: top;
	}
--}}

[class^="{{baseClass}}-"],
[class*="{{baseClass}}-"]{
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: top;
}

{{#each codepoints}}
.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
